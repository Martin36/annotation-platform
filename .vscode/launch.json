  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {"PYTHONPATH":"${workspaceRoot}"},  // modify PYTHONPATH
      // "justMyCode": false
    },
    {
      "name": "Python: Django MGMT Command",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "${fileBasenameNoExtension}"
      ],
      "django": true
    },
    {
      "name": "Python: Django - Import claims",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "import_claims",
        "data/partiledardebatt-22-01-12-filtered.csv"
      ],
      "django": true
    },
    {
      "name": "Python: Django - Test Helpers",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "test",
        "annotations.tests.test_helpers"
      ],
      "django": true
    },
  ]
}